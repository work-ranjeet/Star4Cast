<section id="physicalAppreanceEdit">
    <div class="row">
        <div class="container section page-heading">
            <h1>Dancing Details</h1>
        </div>
    </div>
    <div class="row">
        <div class="container section">
            <div class="section-container">
                <form action="#" method="post" class="form padding-10" role="form">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="profile-edit-label">Auditions & Jobs I'm interested in *</label>
                                <div class="profile-edit-container row">
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Agencies</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="actingCategories" class="" type="checkbox" name="actingCategories" checked="checked" value="32" />
                                        <span class="checkboc-label">Choreographers</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Companies</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Competitions</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr />

                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="profile-edit-label">Styles of dancing</label>
                                <div class="profile-edit-container row">
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Ballet</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="actingCategories" class="" type="checkbox" name="actingCategories" checked="checked" value="32" />
                                        <span class="checkboc-label">Bollywood</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Country</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3 margin-bottom-5">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">Hip Hop</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-md-3 ">
                            <div class="form-group">
                                <label class="profile-edit-label">Dance ability</label>
                                <div class="profile-edit-container">
                                    <select name="musicianBanjoAbility" id="musicianBanjoAbility" class="form-control">
                                        <option value="0">Please select...</option>
                                        <option selected="selected" value="357">Beginner</option>
                                        <option value="358">Intermediate</option>
                                        <option value="359">Expert</option>

                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr />

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="form-group">
                                <label class="profile-edit-label">
                                    Dance school &nbsp;
                                </label>
                                <div class="profile-edit-container" style="padding-left:0px; margin-left:-10px;">
                                    <div class="col-xs-12 col-sm-6 form-group">
                                        <input id="checkbox2" type="checkbox" checked="" class="">
                                        <span class="checkboc-label">I have attended a dance school</span>
                                    </div>
                                </div>


                                <div class="fa fa-border ">
                                    <span style="float: left; padding: 5px; margin-top: -16px; background-color: white;">Enter your most recent school</span>
                                    <div class="col-xs-12  form-group">
                                        <label>School *</label>
                                        <input type="text" placeholder="Enter your latest school name..." class="form-control" />
                                    </div>
                                    <div class="col-xs-12  form-group">
                                        <label>Course name</label>
                                        <input type="text" placeholder="Enter Course name..." class="form-control" />
                                    </div>
                                    <div class="col-xs-12  form-group">
                                        <label>year*</label>
                                        <input type="text" placeholder="Enter year..." class="form-control" />
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="profile-edit-label">Agent</label>
                                <div class="profile-edit-container row">
                                    <div class="col-xs-6 col-sm-3">
                                        <input id="checkbox2" type="radio" checked="" class="">
                                        <span class="checkboc-label">I don't have an agent</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3">
                                        <input id="actingCategories" class="" type="radio" name="actingCategories" checked="checked" value="32" />
                                        <span class="checkboc-label">I want an agent</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3">
                                        <input id="checkbox2" type="radio" checked="" class="">
                                        <span class="checkboc-label">I have an agent</span>
                                    </div>
                                    <div class="col-xs-6 col-sm-3">
                                        &nbsp;
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr />

                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="profile-edit-label">Dancing experience</label>
                                <div class="profile-edit-container">
                                    <textarea style="width:100%; min-height:100px;" class="form-control"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12 text-center padding-15">
                        <a asp-controller="Profile" asp-action="TalentProfileViewAs" id="btnCancle" class="btn-profile-edit-cancle btn btn-danger">Cancle</a>
                        <button id="btnSave" class="btn-profile-edit-save btn btn-success">Save</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

</section>
@section Scripts {

}
